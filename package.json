{
    "name": "boilerplate",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint src --max-warnings=0",
        "prepare": "husky install",
        "test": "jest",
        "test:watch": "yarn test --watch"
    },
    "lint-staged": {
        "src/**/*": [
            "yarn test --bail"
        ]
    },
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "next": "12.2.5",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.18.13",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-typescript": "^7.18.6",
        "@testing-library/react": "^13.3.0",
        "@types/jest": "^29.0.0",
        "@types/node": "18.7.14",
        "@types/react": "18.0.18",
        "@types/react-dom": "18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "babel-jest": "^29.0.1",
        "eslint": "8.23.0",
        "eslint-config-next": "12.2.5",
        "eslint-plugin-react": "^7.31.1",
        "husky": "^8.0.1",
        "jest": "^29.0.1",
        "jest-environment-jsdom": "^29.0.1",
        "lint-staged": "^13.0.3",
        "prettier": "2.7.1",
        "ts-jest": "^28.0.8",
        "typescript": "4.8.2"
    }
}